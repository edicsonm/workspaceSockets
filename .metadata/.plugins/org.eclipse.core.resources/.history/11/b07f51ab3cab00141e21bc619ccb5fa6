import java.io.PrintWriter;
import java.util.HashMap;


public class Clientes {
	private static Clientes instance = null;
	private HashMap<String, DataOutputStream> clientes = new HashMap<String, PrintWriter>();
	
	public static synchronized Clientes getInstance() {
		if (instance == null) {
			instance = new Clientes();
		}
		return instance;
	}
	
	public HashMap<String, PrintWriter> getClientes() {
		return clientes;
	}

	public void setClientes(HashMap<String, PrintWriter> clientes) {
		this.clientes = clientes;
	}

}
